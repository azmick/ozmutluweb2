@using ozmutluweb.Models.Siniflar
@model List<Hakkımızda>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_OzmutluLayout.cshtml";
}

<div class="content">
<div class="wrap">
	<div class="about">
		<h3>Hakkımızda</h3>
		@foreach(var x in Model) {
				<img src="@x.FotoUrl" alt=""/>
				<p>@x.Aciklama</p>
			}
	</div>
</div>
</div>